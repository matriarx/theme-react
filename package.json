{
  "name": "@matriarx/theme-react",
  "version": "0.0.1",
  "description": "Theme ~ React",
  "keywords": [
    "mia",
    "matriarx",
    "javascript",
    "typescript",
    "theme",
    "react"
  ],
  "author": {
    "name": "Mia Matriarx",
    "url": "https://matriarx.io",
    "email": "mia@matriarx.io",
    "community": "https://discord.gg/matriarx"
  },
  "contributors": [],
  "homepage": "https://github.com/matriarx/theme-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matriarx/theme-react.git"
  },
  "bugs": {
    "url": "https://github.com/matriarx/theme-react/issues"
  },
  "license": "MIT",
  "funding": [
    {
      "type": "individual",
      "url": "https://github.com/sponsors/miamatriarx"
    },
    {
      "type": "individual",
      "url": "https://github.com/sponsors/matriarx"
    },
    {
      "type": "patreon",
      "url": "https://patreon/miamatriarx"
    },
    {
      "type": "individual",
      "url": "https://bunq.me/miamatriarx"
    }
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "tag": "latest"
  },
  "config": {},
  "engines": {
    "node": ">= 18",
    "npm": ">= 8"
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "bin/init",
    "lib/**/*.js",
    "lib/**/*.json",
    "types/**/*.ts"
  ],
  "scripts": {
    "start": "node --es-module-specifier-resolution=node --no-warnings lib/index.js",
    "compile": "tsc --project typescript.json",
    "build": "swc src -d lib --copy-files",
    "dev": "swc src -d lib --copy-files -w",
    "review": "tsc --noEmit",
    "lint": "eslint --fix '{src,tests}/**/*.ts' --color --debug --no-error-on-unmatched-pattern",
    "style": "prettier --write '{src,tests}/**/*.ts' --loglevel log --no-error-on-unmatched-pattern",
    "test": "jest --config .jestrc.json --colors --passWithNoTests",
    "qa": "npm run review && npm run lint && npm run style && npm test",
    "type": "tsc --project typescript.json --emitDeclarationOnly",
    "clean": "rm -rf node_modules && rm -rf lib && rm -rf types && rm -rf coverage && rm -rf cache && rm -rf .swc && npm cache clean --force",
    "prepare": "husky install bin && git add bin/pre-commit && git add bin/pre-push"
  },
  "bin": {
    "init": "bin/init"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.204",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "chokidar": "^3.5.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
