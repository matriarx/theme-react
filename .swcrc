{
  "test": ".*.tsx?$",
  "exclude": ["lib", "coverage"],
  "isModule": true,
  "jsc": {
    "baseUrl": ".",
    "paths": {},
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "topLevelAwait": true,
      "importMeta": true,
      "functionBind": true,
      "privateMethod": true,
      "classPrivateProperty": true,
      "exportDefaultFrom": true,
      "exportNamespaceFrom": true,
      "decorators": true,
      "decoratorsBeforeExport": true,
      "jsx": false,
      "tsx": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "optimizer": {
        "simplify": true,
        "jsonify": {
          "minCost": 0
        }
      },
      "react": {
        "runtime": "automatic",
        "importSource": "react",
        "pragma": "h",
        "pragmaFrag": "f",
        "useBuiltins": true,
        "throwIfNamespace": true,
        "development": true,
        "refresh": true
      }
    },
    "target": "es2022",
    "loose": false,
    "keepClassNames": false,
    "externalHelpers": false,
    "preserveAllComments": false,
    "experimental": {
      "keepImportAssertions": true,
      "plugins": []
    }
  },
  "module": {
    "type": "es6",
    "strict": true,
    "strictMode": true,
    "noInterop": false,
    "lazy": false
  },
  "sourceMaps": true,
  "inlineSourcesContent": true,
  "emitSourceMapColumns": true,
  "minify": true
}
